@model List<Product>

<table class="table table-bordered table-hover">
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>@item.Price</td>
			<td>@item.Description</td>
			<td>
				<img src="~/images/@item.ImageURL" style="width:69px" />
			</td>
		</tr>
	}



</table>
